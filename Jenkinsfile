    stages{
        stage('Checkout SCM'){
            steps{
                echo 'Downloading project code'
                }
            }
        stage('Build'){
            steps{
                echo 'Build project'
                }
            }
        stage('Run API tests'){
            steps{
                echo 'Running API tests'
                }
            }
        stage('Run UI tests'){
            steps{
                echo 'Running UI tests'
                }
            }
        }
}